//Print the following pattern for the given N number of rows.


C++ CODE


#include<iostream>
using namespace std;

int main() {
	// Write your code here
	
    
    
    int n;
    cin>>n;
    
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<i+1;j++)
        {
            cout<<"1";
            
        }
        cout<<endl;
    }
}





JAVA CODE


import java.util.*;

public class Main {
	
	public static void main(String[] args) {
		// Write your code here

        
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        
        
     for(int i=0;i<n;i++)
    {
        for(int j=0;j<i+1;j++)
        {
            System.out.print("1");
            
        }
        System.out.println("");
    }
	}
}


